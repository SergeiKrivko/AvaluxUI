<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <Color x:Key="SurfaceColor">#F2F2F2</Color>
            <Color x:Key="CanvasColor">#D4D4D4</Color>
            <Color x:Key="BorderColor">#707070</Color>

            <Color x:Key="Transparent1Color">#10909090</Color>
            <Color x:Key="Transparent2Color">#20909090</Color>

            <Color x:Key="SuccessColor">#529615</Color>
            <Color x:Key="SuccessHoverColor">#3F7310</Color>

            <Color x:Key="WarningColor">#C4821A</Color>
            <Color x:Key="WarningHoverColor">#916013</Color>

            <Color x:Key="DangerColor">#FF4A46</Color>
            <Color x:Key="DangerHoverColor">#E0413E</Color>

            <Color x:Key="PrimaryColor">#328BFF</Color>
            <Color x:Key="PrimaryHoverColor">#3367F2</Color>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <Color x:Key="SurfaceColor">#202020</Color>
            <Color x:Key="CanvasColor">#121212</Color>
            <Color x:Key="BorderColor">#909090</Color>

            <Color x:Key="Transparent1Color">#10606060</Color>
            <Color x:Key="Transparent2Color">#20606060</Color>

            <Color x:Key="SuccessColor">#529615</Color>
            <Color x:Key="SuccessHoverColor">#3F7310</Color>

            <Color x:Key="WarningColor">#C4821A</Color>
            <Color x:Key="WarningHoverColor">#916013</Color>

            <Color x:Key="DangerColor">#C2190C</Color>
            <Color x:Key="DangerHoverColor">#E31D0E</Color>

            <Color x:Key="PrimaryColor">#115992</Color>
            <Color x:Key="PrimaryHoverColor">#0E4978</Color>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>