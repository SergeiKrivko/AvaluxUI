<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="AvaluxUI.AvaluxUIStyles">
    
    <Styles.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="Icons.axaml"></ResourceInclude>
                <ResourceInclude Source="Colors.axaml"></ResourceInclude>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Styles.Resources>
    
    <StyleInclude Source="Styles/Surface.axaml"></StyleInclude>
    <StyleInclude Source="Styles/StackPanel.axaml"></StyleInclude>
    <StyleInclude Source="Styles/Inputs.axaml"></StyleInclude>
    <StyleInclude Source="Styles/Buttons.axaml"></StyleInclude>
    <StyleInclude Source="Styles/Spinner.axaml"></StyleInclude>
    <StyleInclude Source="Styles/Expander.axaml"></StyleInclude>
    <StyleInclude Source="Styles/ListBox.axaml"></StyleInclude>
    <StyleInclude Source="Styles/TabControl.axaml"></StyleInclude>
</Styles>